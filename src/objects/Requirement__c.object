    <label>Requirement</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Case__c</columns>
        <columns>Requirement_Type__c</columns>
        <columns>Status__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <displayFormat>REQ-{0000}</displayFormat>
        <label>Type of Requirement ID</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Requirements</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>CXE_Max_Requirement_Count_Validate</fullName>
        <active>true</active>
        <description>Validate the maximum count of Requirement per Case</description>
        <errorConditionFormula>AND(
ISNEW(), 
Case__r.Number_Of_Type_Reqs__c = 10
)</errorConditionFormula>
        <errorMessage>You can only add 10 Requirements per case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CXE_Status_Deferred_Validate_User</fullName>
        <active>true</active>
        <description>Restrict update of deferred status for G4 and above users only</description>
        <errorConditionFormula>AND(
ISCHANGED(Status__c),
ISPICKVAL(Status__c, &apos;Deferred&apos;),
NOT(CONTAINS($Profile.Name, &apos;G4&apos;)), 
NOT(CONTAINS($Profile.Name, &apos;G3&apos;)),
NOT(CONTAINS($Profile.Name, &apos;G2&apos;)),
NOT(CONTAINS($Profile.Name, &apos;G1&apos;)),
NOT(CONTAINS($Profile.Name, &apos;System Administrator&apos;))
)</errorConditionFormula>
        <errorMessage>Only G4 to G1 Users can change Status to &quot;Deferred&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Documents_Required</fullName>
        <active>false</active>
        <description>Some Requirement Type requires Supporting Document</description>
        <errorConditionFormula>(ISPICKVAL( Requirement_Type__c , &quot;Proof of Identification&quot;) || ISPICKVAL( Requirement_Type__c , &quot;Proof of Occupancy&quot;) || ISPICKVAL( Requirement_Type__c , &quot;Disconnection Waiver/Warranty&quot;) || ISPICKVAL( Requirement_Type__c , &quot;Certificate of Final Electrical Inspection&quot;) || ISPICKVAL( Requirement_Type__c , &quot;Energization Date Agreement Form&quot;) || ISPICKVAL( Requirement_Type__c , &quot;Right of Way Grant&quot;) || ISPICKVAL( Requirement_Type__c , &quot;Undertaking to Submit Pending Document/s&quot;)) &amp;&amp; ISBLANK(TEXT(Supporting_Document__c))</errorConditionFormula>
        <errorDisplayField>Supporting_Document__c</errorDisplayField>
        <errorMessage>Please Select a Supporting Document.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_Only_10_records_Per_Record</fullName>
        <active>false</active>
        <errorConditionFormula>Case__r.Number_Of_Type_Reqs__c = 10</errorConditionFormula>
        <errorMessage>You can only add 10 type of requirements per case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_allow_to_change_back_to_pending</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IsCHANGED( Status__c ),
text(Status__c) = &apos;Pending&apos;,
 TEXT(PRIORVALUE(Status__c)) &lt;&gt; &apos;Pending&apos;,
not(isblank(PRIORVALUE(Status__c)))
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Not allowed to updated status after Pending</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_G4_to_G2_Can_Deffer</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ischanged( Status__c ),
ispickval (Status__c,&apos;Deferred&apos;),
not( contains($UserRole.Name,&apos;G4&apos;)), 
not( contains($UserRole.Name,&apos;G3&apos;)),
not( contains($UserRole.Name,&apos;G2&apos;)),
not( $Profile.Name =&apos;System Administrator&apos;)

)</errorConditionFormula>
        <errorMessage>Only G4 to G2 Users can change to &quot;Deferred&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Remarks_Required_When_Type_is_CFEI</fullName>
        <active>true</active>
        <description>If the Requirement Type is CFEI (Certificate of Final Electrical Inspection) and status is &quot;Submitted&quot;, then the remarks should not be empty.</description>
        <errorConditionFormula>AND(ISPICKVAL(Requirement_Type__c, &quot;Certificate of Final Electrical Inspection&quot;), ISPICKVAL( Status__c , &quot;Submitted&quot;), ISBLANK( Remarks__c ) )</errorConditionFormula>
        <errorMessage>Indicate the CFEI # on the Remarks field when updating the requirement to Submitted.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
