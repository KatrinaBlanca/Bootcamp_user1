    <validationRules>
        <fullName>Active_Products_cant_be_edited</fullName>
        <active>false</active>
        <errorConditionFormula>PRIORVALUE(IsActive) = TRUE &amp;&amp; NOT(LastModifiedDate &lt;&gt; LastModifiedDate) &amp;&amp; NOT( Archived_Product__c = TRUE)</errorConditionFormula>
        <errorMessage>Active products cannot be edited</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Active_cannot_be_unchecked_manually</fullName>
        <active>false</active>
        <description>The following should not be allowed:
1. Go from Active to New (i.e. manually unchecking the Active checkbox)</description>
        <errorConditionFormula>ISCHANGED(IsActive) &amp;&amp;  IsActive  = FALSE</errorConditionFormula>
        <errorDisplayField>IsActive</errorDisplayField>
        <errorMessage>Active Products cannot be changed to new status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Archived_Product_cannot_be_Activated</fullName>
        <active>true</active>
        <description>The following should not be allowed:
2.Go anywhere from Archived</description>
        <errorConditionFormula>(Archived_Product__c = TRUE &amp;&amp;  PRIORVALUE(IsActive) = FALSE)</errorConditionFormula>
        <errorDisplayField>Archived_Product__c</errorDisplayField>
        <errorMessage>Archived Product cannot be activated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Archived_Product_cannot_be_New</fullName>
        <active>true</active>
        <description>The following should not be allowed:
3.Go from Archived to New</description>
        <errorConditionFormula>( PRIORVALUE(Archived_Product__c) = TRUE &amp;&amp; Archived_Product__c = FALSE &amp;&amp; IsActive = FALSE)</errorConditionFormula>
        <errorMessage>Archived Product cannot be changed to new status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Archived_Product_cannot_be_unchecked</fullName>
        <active>true</active>
        <description>The following should not be allowed:
2.Go anywhere from Archived</description>
        <errorConditionFormula>(ISCHANGED( Archived_Product__c ) &amp;&amp;  Archived_Product__c  = FALSE)</errorConditionFormula>
        <errorDisplayField>Archived_Product__c</errorDisplayField>
        <errorMessage>Archived Product checkbox cannot be unchecked manually</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Archived_Products_cant_be_edited</fullName>
        <active>true</active>
        <errorConditionFormula>PRIORVALUE( Archived_Product__c )   = TRUE &amp;&amp; NOT(LastModifiedDate &lt;&gt; LastModifiedDate)</errorConditionFormula>
        <errorMessage>Archived products cannot be edited</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_activate_product_while_creating</fullName>
        <active>false</active>
        <description>we cannot activate product while creating</description>
        <errorConditionFormula>if(ISNEW()&amp;&amp; IsActive ,true,false)</errorConditionFormula>
        <errorMessage>Can&apos;t activate the product while creating</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Score_should_be_betwen_0_and_5</fullName>
        <active>true</active>
        <errorConditionFormula>OR(Score__c  &lt; 0.00,Score__c  &gt; 5.00)</errorConditionFormula>
        <errorMessage>Score can not be less than 0.00 or greater than 5.00</errorMessage>
    </validationRules>
</CustomObject>
