    <validationRules>
        <fullName>CXE_Validate_Mobile_1_Format</fullName>
        <active>true</active>
        <description>Mobile Phone should start with +63 then followed by 10-digit number.</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK(Primary_Mobile__c )), 
NOT(REGEX(Primary_Mobile__c ,&quot;[+][6][3][0-9]{10}&quot;)) 
)</errorConditionFormula>
        <errorMessage>Mobile Phone should start with +63 then followed by 10-digit number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CXE_Validate_Mobile_2_Format</fullName>
        <active>true</active>
        <description>Mobile Phone should start with +63 then followed by 10-digit number.</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK(Secondary_Mobile__c )), 
NOT(REGEX(Secondary_Mobile__c ,&quot;[+][6][3][0-9]{10}&quot;)) 
)</errorConditionFormula>
        <errorMessage>Mobile Phone should start with +63 then followed by 10-digit number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CXE_Validate_Mobile_3_Format</fullName>
        <active>true</active>
        <description>Mobile Phone should start with +63 then followed by 10-digit number.</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK(Alternative_Mobile__c )), 
NOT(REGEX(Alternative_Mobile__c ,&quot;[+][6][3][0-9]{10}&quot;)) 
)</errorConditionFormula>
        <errorMessage>Mobile Phone should start with +63 then followed by 10-digit number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CXE_Validate_Telephone_1_Format</fullName>
        <active>true</active>
        <description>Phone should start with +63 then followed by 1 to 2-digit area code and 7-digit number.</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK(Primary_Telephone__c )), 
