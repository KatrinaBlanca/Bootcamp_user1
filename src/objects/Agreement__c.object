        <errorConditionFormula>OR(ISBLANK(Contract__r.Billing_Preference__c), ISNULL(Contract__r.Billing_Preference__c))</errorConditionFormula>
        <errorMessage>Related Contract must have an address.</errorMessage>
    </validationRules>
