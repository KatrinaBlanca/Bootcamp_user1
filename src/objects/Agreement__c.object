    <validationRules>
        <fullName>CXE_TLN</fullName>
        <active>true</active>
        <description>Service Wire TLN only accepts numeric values.</description>
        <errorConditionFormula>AND( 
OR(ISCHANGED( Service_Wire_TLN__c ), ISNEW()), 
NOT(REGEX( Service_Wire_TLN__c ,&quot;[0-9]*$&quot;))

,$User.By_Pass_Validation__c = FALSE)</errorConditionFormula>
        <errorMessage>Service Wire TLN only accepts numeric values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Contract_Address</fullName>
        <active>false</active>
