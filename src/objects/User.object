    </validationRules>
    <validationRules>
        <fullName>Validation_Create_Portal_User</fullName>
        <active>true</active>
        <description>Validate only RM or System Admin profiles can create External User</description>
        <errorConditionFormula>AND(ISNEW(), $User.By_Pass_Validation__c == false, $Profile.Name &lt;&gt; &quot;System Administrator&quot;, $Profile.Name &lt;&gt; &quot;Super User 2&quot;, $UserRole.Name &lt;&gt; &quot;Biz Partner Manager&quot;, $UserRole.Name &lt;&gt; &quot;Biz Zone Officer&quot;, $UserRole.Name &lt;&gt; &quot;CBG Relationship Manager&quot;)</errorConditionFormula>
        <errorMessage>Only System Administrator or RM Roles can create portal users</errorMessage>
    </validationRules>
