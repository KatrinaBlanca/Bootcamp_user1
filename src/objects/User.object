        <fullName>Reset_Password_Portal_User</fullName>
        <availability>online</availability>
        <description>Reset Password for Portal User (to tackle limitation on standard Reset Password)</description>
        <displayType>link</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Reset Password Portal User</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)} 

if(window.confirm(&quot;Reset password for this user?&quot;)){ 
var query = &quot;select Id, CXE_ForgotPasswordFlag__c from User where Id = &apos;{!User.Id}&apos; limit 1&quot;; 

var result = sforce.connection.query(query); 
var records = result.getArray(&quot;records&quot;); 

var myObj = records[0]; 

myObj.CXE_ForgotPasswordFlag__c = true; 

var results = sforce.connection.update([myObj]); 

if (results[0].success === &quot;false&quot;){ 
alert(result[0].errors.message); 
} 
else{ 
alert(&quot;Reset password email sent successfully&quot;); 
} 
}</url>
    </webLinks>
