    <rules>
        <fullName>CAPS Billing Address Always</fullName>
        <actions>
            <name>CAPS_Billing_Addr_Street_Name</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>CAPS_Billing_Addr_Street_No</name>
            <type>FieldUpdate</type>
        </actions>
