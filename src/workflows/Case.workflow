        <description>CAPS Billing Address always</description>
        <formula>AND ((ISCHANGED(Billing_Address_Street_Name__c) &amp;&amp; !ISBLANK(Billing_Address_Street_Name__c)) ||  (ISCHANGED(Billing_Address_Street_No__c) &amp;&amp; !ISBLANK(Billing_Address_Street_No__c)) ||  (ISCHANGED(Billing_Address_Unit_No__c) &amp;&amp; !ISBLANK(Billing_Address_Unit_No__c)) ||  (ISCHANGED(Billing_Address_Subdivision__c) &amp;&amp; !ISBLANK(Billing_Address_Subdivision__c)) ,  ( RecordType.Name = &apos;Customer Community&apos; ||   RecordType.Name = &apos;Service Application (Admin)&apos; ||   RecordType.Name = &apos;Customer Community Record Type&apos; ||   RecordType.Name = &apos;Service Application&apos;))</formula>
